declare module mod1;
<<<driver Program>>>
start
    declare v, w, r :integer;
    get_value(v);
    w:=5;
    declare m:real;
    [r,m] := use module mod1 with parameters v, w;
    print(r);
    print(m);
end

<<module mod1>>
takes input [a: integer, b: integer];
returns [x: integer, abc: real];
start
    declare c: real;
    declare i, j: integer;
    i..j;
    c:=10.4e-6;
    x:=a+b-10;
    abc:=b/5+c;
end